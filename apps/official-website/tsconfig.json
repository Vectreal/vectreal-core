    "allowJs": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "lib": ["ESNext", "DOM", "DOM.Iterable", "DOM.AsyncIterable"],
    "types": ["vite/client", "node"],
    "baseUrl": "./",
    "paths": {
      "@vctrl/hooks": ["../../packages/hooks/*"],
      "@vctrl/viewer": ["../../packages/viewer/*"],
